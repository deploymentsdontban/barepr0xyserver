{
  "version": 2,
  "functions": [
    {
      "src": "src/main.py",
      "runtime": "python3.9",
      "handler": "main.handler"
    }
  ],
  "routes": [
    {
      "src": "/**",
      "dest": "dist/index.html"
    }
  ],
  "trailingSlash": true,
  "rewrites": [
    {
      "src": "**/*.**",
      "dst": "dist/index.html"
    }
  ]
}
